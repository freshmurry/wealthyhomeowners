<div class="row">
  <div class="col-md-3">
    <%= render 'home_menu' %>
  </div>
  <div class="col-md-9">
    <div class="panel panel-default">

      <div class="panel-heading">
        Location
      </div>

      <div class="panel-body">
        <div class="container">
          <%= form_for @home do |f| %>
            
            <div class="container">
              <div class="row">
                <div class="form-group">
                  <label>Address</label>
                  <%= f.text_field :address, placeholder: "Where is your home located?", class: "form-control", required: true, id: "autoaddress" %>
                </div>
              </div>
            </div>
            
            <div class="text-center">
              <%= f.submit "Save", class: "btn btn-form" %>
            </div>

          <% end %>
        </div>
      </div>

    </div>
  </div>
</div>

<script>
  $(function() {
    $("#autoaddress").geocomplete();
  })
</script>