$('#photos').html("<%= j render 'photos_list' %>")
<% if @home.photos.blank? %>
  $('#photo_check').hide();
<% end %>

<% if !@home.active && !@home.price.blank? && !@home.listing_name.blank? && !@home.photos.blank? && !@home.location_type.blank? %>
    $('#publish_button').attr('disabled', false);
<% else %>
    $('#publish_button').attr('disabled', true);
<% end %>